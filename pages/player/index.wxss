.normal-player {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 150;
  background: #141e30;
}

.background {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  opacity: 0.6;
  -webkit-filter: blur(15px);
  filter: blur(15px);
}

.background image {
	width: 100%;
	height: 100%;
}

.middle {

}

.middle .cd-wrapper {
	width: 540rpx;
	height: 540rpx;
	border-radius: 50%;
	box-shadow: 20rpx 20rpx 20rpx 20rpx rgba(255, 255, 255, 0.5);
	margin-top: 200rpx;
	margin: 200rpx auto;
}

.middle .cd-wrapper .cd-img {
	width: 440rpx;
	height: 440rpx;
	border-radius: 50%;
}